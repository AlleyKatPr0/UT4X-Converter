<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane id="AnchorPane" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.xtx.ut4converter.ui.ConversionViewController">
    <children>
        <TableView tableMenuButtonVisible="true" fx:id="convLogTableView"  GridPane.columnIndex="0" GridPane.columnSpan="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="0" GridPane.vgrow="ALWAYS">
            <columns>
                <TableColumn text="Time" fx:id="logTime"></TableColumn>
                <TableColumn text="Type" fx:id="logLevel"></TableColumn>
                <TableColumn text="Message" fx:id="logMsg"></TableColumn>
            </columns>
        </TableView>
    </children>
</GridPane>
